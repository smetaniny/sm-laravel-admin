{
    "name": "smetaniny/sm-laravel-image",
    "description": ":Обработка изображений laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Smetanin Sergey",
            "email": "sm.sergey.v@yandex.ru",
            "homepage": "https://smetaniny.ru/"
        }
    ],
    "homepage": "https://github.com/smetaniny/sm-laravel-admin",
    "keywords": [
        "Laravel",
        "SmLaravelAdmin"
    ],
    "require": {
        "illuminate/support": "~9"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~7"
    },
    "autoload": {
        "psr-4": {
            "Smetaniny\\SmLaravelAdmin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Smetaniny\\SmLaravelAdmin\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.1-dev"
        },
        "laravel": {
            "providers": [
                "Smetaniny\\LaravelImage\\SMLaravelAdminServiceProvider"
            ],
        }
    },
    "minimum-stability": "stable",
}
